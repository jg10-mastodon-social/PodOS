{
  "name": "@pod-os/core",
  "version": "0.7.0",
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "lib/",
    "dist/",
    "types/"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint ./src",
    "build:bundle": "rimraf lib && esbuild src/index.ts --outfile=lib/index.js --bundle --target=esnext --global-name=PodOS",
    "build:esm": "rimraf dist && esbuild src/index.ts --outdir=dist --bundle --splitting --format=esm",
    "build:types": "rimraf types && tsc --emitDeclarationOnly --outDir types",
    "build:watch": "npm run build:esm -- --watch",
    "build": " npm run build:bundle && npm run build:types && npm run build:esm"
  },
  "keywords": [
    "Solid"
  ],
  "author": "Angelo Veltens",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.5.0",
    "@types/jest-when": "^3.5.2",
    "@types/sparqljs": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "esbuild": "^0.16.17",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "jest-when": "^3.5.2",
    "prettier": "^2.8.6",
    "rdf-namespaces": "^1.10.1",
    "rimraf": "^3.0.2",
    "sparqljs": "^3.6.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@inrupt/solid-client-authn-browser": "^1.13.4",
    "buffer": "^6.0.3",
    "rdflib": "^2.2.31",
    "url": "^0.11.0"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pod-os/pod-os.git"
  },
  "bugs": {
    "url": "https://github.com/pod-os/pod-os/issues"
  },
  "homepage": "https://github.com/pod-os/pod-os#readme",
  "description": ""
}
